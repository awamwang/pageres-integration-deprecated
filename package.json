    "sinon": "^1.17.2",
    "execa": "^0.4.0",
    "path-exists": "^3.0.0",
    "xo": "*"
  },
  "babel": {
    "plugins": [
      "transform-async-to-generator",
      "transform-runtime",
      "transform-flow-strip-types",
      "add-module-exports"
    ],
    "presets": [
      "es2015-node4"
    ],
    "sourceMaps": "inline"
  },
  "xo": {
    "esnext": true,
    "parser": "babel-eslint",
    "rules": {
      "generator-star-spacing": "off"
    }
  },
  "directories": {
    "test": "test"
  }
}
