    "fs-write-stream-atomic": "^1.0.2",
    "get-res": "^3.0.0",
    "lodash.template": "^4.0.1",
    "log-symbols": "^1.0.2",
    "mem": "^1.1.0",
    "mkdirp": "^0.5.0",
    "pify": "^2.3.0",
    "plur": "^2.0.0",
    "protocolify": "^1.0.0",
    "rimraf": "^2.2.8",
    "screenshot-stream": "^4.1.0",
    "viewport-list": "^5.0.1",
    "array-differ": "^1.0.0",
